        {
            "name": "Generate Explantion food",
            "type": "python",
            "request": "launch",
            "python": "/mnt/abka03/.conda/road/bin/python",
            "program": "experiments/explanation_generation/ExplanationGeneration_food.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--test=True",
		        "--gpu=",
		        "--input_path=/mnt/abka03/road_data/food101/image/food-101",
		        "--save_path=/mnt/abka03/road_data/food101/image/food-101/expl/",
		        "--model_path=/mnt/abka03/road_data/food101/weight/resnet50model.pth",
		        "--batch_size=1",
		        "--expl_method=ig_sg"
            ] 
        },

        {
            "name": "ROAD No Retain (food)",
            "type": "python",
            "request": "launch",
            "python": "/mnt/abka03/.conda/road/bin/python",
            "program": "experiments/food101/NoRetrainingMethod.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "args": [
                "--gpu=",
                "--data_path=/workspaces/data/food101/image/food-101",
                "--expl_path=/workspaces/data/food101/image/food-101/expl",
